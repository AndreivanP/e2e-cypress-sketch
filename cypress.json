{
    "baseUrl": "https://www.sketch.com/",
    "video": false,
    "defaultCommandTimeout": 10000,
    "viewportWidth": 1440,
    "viewportHeight": 900,
    "chromeWebSecurity": false,
    "reporter": "mochawesome",
    "filterLoad": "proxy/companies",
    "screenshotsFolder": "mochawesome-report/assets",
    "retries":{
      "runMode": 1,
      "openMode": 1
    },
    "reporterOptions": {
        "reportDir": "cypress/reports",
        "overwrite": false,
        "html": false,
        "json": true
     },
    "env": {
            "AUTH_URL": "https://graphql.sketch.cloud/oauth/token"
          }
  }
